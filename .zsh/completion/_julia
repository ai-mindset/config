#compdef julia

_arguments \
  '-v[Display version information]' \
  '--version[Display version information]' \
  '-h[Print command-line options]' \
  '--help[Print command-line options]' \
  '--help-hidden[Print uncommon options not shown by -h]' \
  '--project=[Set directory as the active project/environment]:project directory:_files -/' \
  '-J[Start up with the given system image file]:system image:_files' \
  '--sysimage=[Start up with the given system image file]:system image:_files' \
  '-H[Set location of julia executable]:home directory:_files -/' \
  '--home=[Set location of julia executable]:home directory:_files -/' \
  '--startup-file=[Load startup file]:option:(yes no)' \
  '--handle-signals=[Enable or disable Julia'\''s default signal handlers]:option:(yes no)' \
  '--sysimage-native-code=[Use native code from system image]:option:(yes no)' \
  '--compiled-modules=[Enable or disable incremental precompilation of modules]:option:(yes no existing strict)' \
  '--pkgimages=[Enable or disable usage of native code caching]:option:(yes no existing)' \
  '-e[Evaluate expression]:expression:' \
  '--eval=[Evaluate expression]:expression:' \
  '-E[Evaluate expression and display the result]:expression:' \
  '--print=[Evaluate expression and display the result]:expression:' \
  '-m[Run entry point of Package]:module:' \
  '--module=[Run entry point of Package]:module:' \
  '-L[Load file immediately on all processors]:file:_files' \
  '--load=[Load file immediately on all processors]:file:_files' \
  '-t[Enable threads]:thread option:' \
  '--threads=[Enable threads]:thread option:' \
  '--gcthreads=[Use threads for GC]:thread option:' \
  '-p[Launch additional local worker processes]:number:' \
  '--procs=[Launch additional local worker processes]:number:' \
  '--machine-file=[Run processes on hosts listed in file]:file:_files' \
  '-i[Interactive mode]' \
  '--interactive[Interactive mode]' \
  '-q[Quiet startup: no banner, suppress REPL warnings]' \
  '--quiet[Quiet startup: no banner, suppress REPL warnings]' \
  '--banner=[Enable or disable startup banner]:option:(yes no short auto)' \
  '--color=[Enable or disable color text]:option:(yes no auto)' \
  '--history-file=[Load or save history]:option:(yes no)' \
  '--depwarn=[Enable or disable deprecation warnings]:option:(yes no error)' \
  '--warn-overwrite=[Enable or disable method overwrite warnings]:option:(yes no)' \
  '--warn-scope=[Enable or disable warning for ambiguous top-level scope]:option:(yes no)' \
  '-C[Limit usage of CPU features]:target:' \
  '--cpu-target=[Limit usage of CPU features]:target:' \
  '-O[Set the optimization level]:level:(0 1 2 3)' \
  '--optimize=[Set the optimization level]:level:(0 1 2 3)' \
  '--min-optlevel=[Set a lower bound on the optimization level]:level:(0 1 2 3)' \
  '-g[Set the level of debug info generation]:level:(0 1 2)' \
  '--debug-info=[Set the level of debug info generation]:level:(0 1 2)' \
  '--inline=[Control whether inlining is permitted]:option:(yes no)' \
  '--check-bounds=[Emit bounds checks]:option:(yes no auto)' \
  '--math-mode=[Floating point semantics]:option:(ieee user)' \
  '--code-coverage=[Count executions of source lines]:option:(none user all)' \
  '--track-allocation=[Count bytes allocated by each source line]:option:(none user all)' \
  '--bug-report=[Launch a bug report session]:kind:' \
  '--heap-size-hint=[Forces garbage collection if memory usage is higher]:size:' \
  '*:script or expression:_files'